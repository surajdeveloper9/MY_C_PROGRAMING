// sum of arrays
#include <stdio.h>
int main() {
    int sum=0,n;
    
    printf("enter the number of elments");
    scanf("%d",&n);
    int arr[n];
    printf("enter the elements:");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
    printf("sum=%d\n",sum);
}
